    <SaveRate tree="option" type="double" mode="basic" dynamic="false">1000</SaveRate>
    <Data type="Data" asbtype="Data" tree="object" mode="basic" dynamic="false">
     <updateVar tree="option" type="std::string" mode="basic" dynamic="false">Cons</updateVar>
    </Data>
