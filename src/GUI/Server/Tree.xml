<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<XCFcase type="XCFcase" abstype="XCFcase" tree="object" dynamic="false" mode="basic">
 <Simulator type="Simulator" abstype="Simulator" tree="object" dynamic="false" mode="basic">
