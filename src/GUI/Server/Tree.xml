     <Def tree="option" type="std::string" mode="basic" dynamic="false">if(y&gt;0.5,0.5,1.) if(y&gt;0.5,1.67332,2.83972) 0.0 if(y&gt;0.5,3.425,6.532)</Def>
     <Vars tree="option" type="std::string" mode="basic" dynamic="false">x y</Vars>
     <applyTRS tree="option" type="std::string" mode="basic" dynamic="false">SuperInlet</applyTRS>
    </Jet1>
    <Jet2 type="Jet2" abstype="Jet2" tree="object" mode="advanced" dynamic="false">
