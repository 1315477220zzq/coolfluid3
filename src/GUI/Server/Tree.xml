    <InField type="InField" abstype="InField" tree="object" mode="advanced" dynamic="false">
     <Def tree="option" type="std::string" mode="basic" dynamic="false">0.5 1.67332 0.0 3.425</Def>
     <Vars tree="option" type="std::string" mode="basic" dynamic="false">x y</Vars>
     <applyTRS tree="option" type="std::string" mode="basic" dynamic="false">InnerCells SuperOutlet</applyTRS>
    </InField>
