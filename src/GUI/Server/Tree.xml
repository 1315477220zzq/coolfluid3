     <FileName tree="option" type="std::string" mode="basic" dynamic="false">jets2D.CFmesh</FileName>
     <builderName tree="option" type="std::string" mode="basic" dynamic="false">RDS</builderName>
     <polyTypeName tree="option" type="std::string" mode="basic" dynamic="false">Lagrange</polyTypeName>
    </Data>
   </CFmeshFileReader>
   <Default type="Default" abstype="Default" tree="object" dynamic="false" mode="basic">
     <PhysicalModelType tree="option" type="std::string" mode="basic" dynamic="false">Euler2D</PhysicalModelType>
     <listTRS tree="option" type="std::string" mode="basic" dynamic="false">InnerCells SuperInlet SuperOutlet</listTRS>
   </Default>
   <FluctuationSplit type="FluctuationSplit" abstype="FluctuationSplit" tree="object" dynamic="false" mode="advanced">
    <BcComds tree="option" type="std::string" mode="basic" dynamic="false">SuperInlet SuperOutlet</BcComds>
